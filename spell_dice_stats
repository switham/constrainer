time ./spell_dice.py -c -m -v $1 >/tmp/spell_dice.out
echo " "
grep "===== pop " /tmp/spell_dice.out |sort -n --key=3 |uniq -c
echo " "
$(tail -2 /tmp/spell_dice.out
echo $(grep try /tmp/spell_dice.out |wc -l) guesses
echo " "
grep "===== solution " /tmp/spell_dice.out|sort -n --key=4 |uniq -f 3 -c
